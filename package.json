{
  "name": "boilerplate-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "asyncapi:generate": "ag src/docs/asyncapi/document.json @asyncapi/html-template -o dist/docs/asyncapi/page",
    "prebuild": "rimraf dist && npm run asyncapi:generate",
    "build": "nest build",
    "pipeline:build": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run asyncapi:generate",
    "start": "TZ=UTC nest start",
    "start:dev": "npm start -- --watch",
    "start:debug": "npm start -- --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run test:unit && npm run test:int && npm run test:component",
    "test:debug": "TZ=UTC node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "TZ=UTC jest --config ./jest/jest.config.js",
    "test:int": "TZ=UTC jest --runInBand --detectOpenHandles --config ./jest/jest.int-config.js",
    "test:component": "TZ=UTC jest --runInBand --config ./jest/jest.component-config.js",
    "test:e2e": "TZ=UTC jest --runInBand --config ./jest/jest.e2e-config.js",
    "pipeline:test": "npm run pipeline:test:unit && npm run pipeline:test:int && npm run pipeline:test:component",
    "pipeline:test:unit": "npm run test:unit -- --coverage",
    "pipeline:test:int": "TYPEORM_HOST=mysql npm run test:int -- --coverage",
    "pipeline:test:component": "TYPEORM_HOST=mysql npm run test:component",
    "typeorm": "DOTENV_CONFIG_PATH=environments/.env node -r dotenv/config ./node_modules/typeorm/cli-ts-node-commonjs.js -d src/modules/configuration/database/datasource.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.3.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/terminus": "^9.1.0",
    "@nestjs/typeorm": "^9.0.0",
    "@newrelic/winston-enricher": "4.0.0",
    "@sanardigital/traceability-core": "^1.1.0",
    "@sanardigital/traceability-middleware": "^1.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dotenv": "^16.0.1",
    "express": "4.18.1",
    "express-openapi-validator": "^4.13.8",
    "mysql2": "^2.3.3",
    "newrelic": "^9.0.0",
    "openapi-types": "12.0.0",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "swagger-ui-express": "4.5.0",
    "typeorm": "^0.3.10",
    "unleash-client": "^3.15.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@asyncapi/generator": "1.9.5",
    "@asyncapi/html-template": "0.24.10",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.0.3",
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "@types/passport": "^1.0.9",
    "@types/passport-http": "^0.3.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-boundaries": "^2.10.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-when": "^3.5.1",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
